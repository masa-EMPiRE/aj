<div class="title">
    <h1><%= @farm.name %></h1>
</div>
<div>
  <div class="description">
    <%= @farm.description %>
</div>
<div>
  <div>
    <%= image_tag @farm.image_url, :size =>'320x320' %>
  </div>
  <div class="event">
    <h3>Event List</h3>
    <% @farm.events.each do |event| %>
      <%= link_to event.eventname, new_event_event_entry_path(event) %>
    <% end %>
  </div>
</div>